{
  "name": "Urban Testing",
  "data": {
    "type": "CSVDataManager",
    "path": "./UrbanSound8K",
    "format": "audio",
    "loader": {
      "shuffle": true,
      "batch_size": 24,
      "num_workers": 4,
      "drop_last": true
    },
    "splits": {
      "train": [1, 2, 3, 4, 5, 6, 7, 8, 9],
      "val": [10]
    }
  },
  "transforms": {
    "type": "AudioTransforms",
    "args": {
      "channels": "avg",
      "noise": [0.3, 0.001],
      "crop": [0.4, 0.25],
      "augmentation": [0.3, 0.01]
    }
  },
  "optimizer": {
    "type": "RMSprop",
    "args": {
      "lr": 0.005,
      "alpha": 0.95,
      "eps": 1e-08,
      "weight_decay": 0.01,
      "momentum": 0,
      "centered": false
    }
  },
  "lr_scheduler": {
    "type": "StepLR",
    "args": {
      "step_size": 2,
      "gamma": 0.75
    }
  },
  "model": {
    "type": "AudioRNN"
  },
  "train": {
    "loss": "nll_loss",
    "epochs": 20,
    "save_dir": "saved_cv/",
    "save_p": 1,
    "verbosity": 2,
    "monitor": "min val_loss",
    "early_stop": 5,
    "tbX": true
  },
  "metrics": "classification_metrics"
}
